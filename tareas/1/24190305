#include <iostream>
int factorial(int n) {
  if (n == 0) {
    return 1;
  } else {
    return n * factorial(n - 1);
  }
}
int main() {
  int number;
  std::cout << "Ingresa un nÃºmero: ";
  std::cin >> number;
  std::cout << "El factorial de " << number << " es: " << factorial(number) << std::endl;
  return 0;
}
